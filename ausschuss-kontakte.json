  "n8n_beispiele": {
    "beschreibung": "JSONPath-Beispiele für n8n Workflows",
    "queries": {
      "alle_vertreter": {
        "path": "$.ausschuss.vertreter[*].vertreter[*]",
        "beschreibung": "Gibt alle Ausschussvertreter zurück"
      },
      "alle_emails": {
        "path": "$.ausschuss.vertreter[*].vertreter[*].email",
        "beschreibung": "Gibt alle E-Mail-Adressen zurück"
      },
      "alle_telefonnummern": {
        "path": "$.ausschuss.vertreter[*].vertreter[*].telefon",
        "beschreibung": "Gibt alle Telefonnummern zurück"
      },
      "nur_stweg_3": {
        "path": "$.ausschuss.vertreter[?(@.stweg_nummer == 3)].vertreter[*]",
        "beschreibung": "Gibt nur Vertreter von STWEG 3 zurück"
      },
      "tiefgarage_vertreter": {
        "path": "$.ausschuss.vertreter[?(@.stweg_nummer == 8)].vertreter[*]",
        "beschreibung": "Gibt Vertreter der Tiefgarage (STWEG 8) zurück"
      },
      "nach_nachname": {
        "path": "$.ausschuss.vertreter[*].vertreter[?(@.nachname == 'Müller')]",
        "beschreibung": "Suche nach spezifischem Nachnamen"
      },
      "alle_wohngebäude": {
        "path": "$.ausschuss.vertreter[?(@.stweg_typ == 'Wohngebäude')].vertreter[*]",
        "beschreibung": "Nur Vertreter von Wohngebäuden (STWEG 1-7)"
      }
    }
  }
